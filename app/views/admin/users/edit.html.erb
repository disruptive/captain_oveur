<h1>Edit User <%= @user.id %></h1>

<%= render(:partial => "admin/users/form") %>

<%= button_to "Activate Admin", activate_admin_admin_user_path(@user), { :method => :put, :confirm => "Are you sure?", :id => "activate_admin_user_#{@user.id}" } %>

<%= button_to "Deactivate Admin", deactivate_admin_admin_user_path(@user), { :method => :put, :confirm => "Are you sure?", :id => "deactivate_admin_user_#{@user.id}" } %>